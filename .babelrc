{
  "env": {
    "production": {
      "presets": [
        ["next/babel", {
          "preset-env": {
            "targets": {
              "browsers": [">1% in us", "firefox esr", "not ie > 0"],
              "node": "current"
            }
          }
        }]
      ]
    },
    "development": {
      "presets": [
        ["next/babel", {
          "preset-env": {
            "targets": {
              "browsers": "last 1 Chrome version",
              "node": "current"
            }
          }
        }]
      ]
    },
    "test": {
      "presets": [
        ["next/babel", {
          "preset-env": {
            "modules": "commonjs",
            "targets": {
              "browsers": "last 1 Chrome version",
              "node": "current"
            }
          }
        }]
      ]
    }
  },
  "plugins": [
    "inline-dotenv",
    ["module-resolver", {
      "root": ["./"]
    }]
  ]
}
