{
  "name": "web-skeleton",
  "version": "1.0.0",
  "description": "Blazing Edge skeleton for React/Redux web applications",
  "scripts": {
    "lint": "eslint . --ignore-pattern '/node_modules/*'",
    "test:only": "jest",
    "test": "run-s -s lint test:only",
    "snapshots": "jest -u",
    "build": "next build",
    "dev": "node server.js",
    "start": "NODE_ENV=production node server.js",
    "heroku-postbuild": "npm run build"
  },
  "author": "Blazing Edge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blazing-edge-labs/web-skeleton"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^23.4.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "exports-loader": "^0.6.4",
    "fetch-mock": "^5.5.0",
    "file-api": "^0.10.4",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/polyfill": "7.0.0-beta.42",
    "autoprefixer": "^6.5.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-hmre": "^1.1.1",
    "dotenv-safe": "^4.0.4",
    "draft-js": "^0.10.5",
    "es6-promise": "^4.0.5",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "hoist-non-react-statics": "^2.3.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-lib-hook": "^1.1.0",
    "next": "^6.1.1",
    "next-redux-wrapper": "^1.3.4",
    "next-routes": "^1.0.40",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-select": "^1.0.0-rc.10",
    "redux": "^3.7.2",
    "redux-form": "7.0.4",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.1.0",
    "style-loader": "^0.19.0",
    "universal-cookie": "^2.1.0",
    "universal-cookie-express": "^2.1.0",
    "validator": "^6.1.0"
  },
  "engines": {
    "node": "8",
    "yarn": "1"
  },
  "jest": {
    "cacheDirectory": "./.test/cache",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!setupTests.js",
      "!scripts/**",
      "!next-style-loader/**"
    ],
    "coverageDirectory": "./.test/coverage",
    "coverageThreshold": {
      "global": {
        "statements": 60
      }
    },
    "setupFiles": [
      "./setupTests.js"
    ],
    "testEnvironment": "node",
    "testRegex": "\\btest\\.js$",
    "timers": "fake",
    "rootDir": ".",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
