{
  "name": "web-skeleton",
  "version": "1.0.0",
  "description": "ECP skeleton for React/Redux web application",
  "scripts": {
    "lint": "eslint . --ignore-pattern '/node_modules/*'",
    "test": "jest",
    "snapshots": "jest -u",
    "build": "next build",
    "dev": "node server.js",
    "start": "NODE_ENV=production node server.js",
    "heroku-postbuild": "npm run build"
  },
  "author": "ECP",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EastCoastProduct/web-skeleton"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.28.7",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "exports-loader": "^0.6.4",
    "express": "^4.14.0",
    "fetch-mock": "^5.5.0",
    "file-api": "^0.10.4",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^17.0.0",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.2.1",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-polyfill": "^6.16.0",
    "dotenv-safe": "^4.0.4",
    "draft-js": "^0.9.1",
    "es6-promise": "^4.0.5",
    "font-awesome": "^4.7.0",
    "hoist-non-react-statics": "^2.3.1",
    "next": "^3.2.2",
    "next-redux-wrapper": "^1.3.4",
    "next-routes": "^1.0.40",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-select": "^1.0.0-rc.10",
    "redux": "^3.7.2",
    "redux-form": "7.0.4",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.1.0",
    "store": "^1.3.20",
    "style-loader": "^0.19.0",
    "validator": "^6.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "engines": {
    "node": "8"
  },
  "jest": {
    "cacheDirectory": "./.test/cache",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!setupTests.js"
    ],
    "coverageDirectory": "./.test/coverage",
    "coverageThreshold": {
      "global": {
        "statements": 60
      }
    },
    "setupFiles": [
      "./setupTests.js"
    ],
    "testEnvironment": "node",
    "testRegex": "\\btest\\.js$",
    "timers": "fake",
    "rootDir": ".",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
